<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ygl.mapper.WaterFilmMapper">
    <insert id="WaterFilmIntoSQL" parameterType="WaterFilm" useGeneratedKeys="false">
        insert into water_film (code,create_time,water_depth,risk_grade)
        values (
            #{code},
            #{createTime},
            #{waterDepth},
            #{riskGrade}
            )
    </insert>
</mapper>